{
	// 顏色顯示
	"colors.filetypes": ["*"],

	// 顯示擴展的 Emmet 縮寫作為建議。	"always"
	"emmet.showExpandedAbbreviation": "always",

	// 顯示 Emmet 縮寫作為建議。不適用於樣式表或 emmet.showExpandedAbbreviationis "never"。	true
	"emmet.showAbbreviationSuggestions"	: true,

	// 在默認情況下不支持的語言中啟用 Emmet 縮寫。在此處添加語言和 Emmet 支持的語言之間的映射。例如：{"vue-html": "html", "javascript": "javascriptreact"}	{}
	"emmet.includeLanguages": {},

	// Emmet 片段中使用的變量	{}
	"emmet.variables": {},

	// 為指定的語法定義配置文件或使用您自己的具有特定規則的配置文件。	{}
	"emmet.syntaxProfiles": {},

	// 不應擴展 Emmet 縮寫的語言數組。	["markdown"]
	"emmet.excludeLanguages": ["markDown"],

	// 包含 Emmet 配置文件和片段的文件夾的路徑。	null
	"emmet.extensionsPath": "null",

	// 將 Emmet 完成項顯示為片段類型。	true
	"emmet.showSuggestionsAsSnippets": true,

	// false，將解析整個文件以確定當前位置是否對擴展 Emmet 縮寫有效。true，僅解析 CSS/SCSS/Less 文件中當前位置周圍的內容。	true
	"emmet.optimizeStylesheetParsing": true,

	// Emmet 補全源的優先級，改為100高於語言服務器的優先級。	3
	"emmet.priority": 3,

	// 用於修改 Emmet 的某些操作和解析器的行為的首選項。	{}
	"emmet.preferences": {},

	"eslint.autoFixOnSave": true,

	"pairs.disableLanguages": [],
	"pairs.enableCharacters": ["(", "[", "{", "<", "'", "\"", "`"],
	// imap for backspace to remove paired characters, default: true, 
	"pairs.enableBackspace": true  

}
